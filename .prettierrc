{
    "trailingComma": "none",
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",

    "importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/ui/",
		"/component/",
		"/styles",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}